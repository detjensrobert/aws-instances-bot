{
  "name": "AWSInstancesBot",
  "version": "1.0.0",
  "description": "Discord bot written in Node.js to start and stop AWS EC2 instances",
  "main": "bot.js",
  "scripts": {
    "once": "node .",
    "start": "pm2 start . --name AWSInstances",
    "stop": "pm2 stop .",
    "restart": "pm2 restart .",
    "fix": "npx eslint . --fix",
    "lint": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detjensrobert/aws-instances-bot.git"
  },
  "author": "Robert Detjens / wholewheatbagels",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/detjensrobert/aws-instances-bot/issues"
  },
  "homepage": "https://github.com/detjensrobert/aws-instances-bot#readme",
  "dependencies": {
    "discord.js": "^12.0.2",
    "pm2": "^4.2.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  }
}
